.box {
  width: 694rpx;
  height: 266rpx;
  background: #fff;
  margin: 0 auto;
  border-radius: 15rpx;
  -webkit-box-shadow: 0 2rpx 20rpx #e3e3e3;
          box-shadow: 0 2rpx 20rpx #e3e3e3;
