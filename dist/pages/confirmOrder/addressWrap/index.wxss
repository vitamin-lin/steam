
@-webkit-keyframes shake {
  from,
  to {
    -webkit-transform: translate3d(20rpx, 0, 0);
            transform: translate3d(20rpx, 0, 0); }
  10%,
  30%,
  50%,
  70%,
  90% {
    -webkit-transform: translate3d(-20rpx, 0, 0);
            transform: translate3d(-20rpx, 0, 0); }
  20%,
  40%,
  60%,
  80% {
    -webkit-transform: translate3d(20rpx, 0, 0);
            transform: translate3d(20rpx, 0, 0); } }

@keyframes shake {
