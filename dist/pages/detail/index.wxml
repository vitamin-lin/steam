<block>
    <view class="page-wrap" style="{{anonymousState__temp5}}">
        <curtain data="{{_params}}" __triggerObserer="{{ _triggerObserer }}" path="{{$router}}" bindonchangestaus="onChangeStaus" mid="{{mid}}" __fn_onChangeStaus="{{true}}"></curtain>
        <view class="cs-wrap" bindtap="clickCs">
            <button class="cs-cn" open-type="contact">
                <image src="{{iconCS}}" mode="widthFix" class="cs-icon"></image>
            </button>
        </view>
        <view class="cart-wrap">
            <view class="cart-cn" catchtap="anonymousFunc1">
                <view class="{{isIconCart ? 'dot' : 'dot hide'}}"></view>
                <image class="cart-pic" src="{{iconCart}}"></image>
            </view>
        </view>
        <view class="detail-wrap">
            <view class="head-pic">
                <swiper class="index-swiper" indicator-color="#999" indicator-active-color="#333" circular="{{true}}" indicator-dots="{{true}}" autoplay="{{true}}">
                    <swiper-item wx:key="index" wx:for="{{swItemArr}}" wx:for-item="url" wx:for-index="index">
                        <view class="index-swiper__item">
                            <image src="{{url}}" class="index-swiper__itemImg"></image>
                        </view>
                    </swiper-item>
                </swiper>
                <view class="rule-wrap" catchtap="anonymousFunc2"></view>
            </view>
            <view class="profile-cn">
                <view class="profile">
                    <view class="p-text">
                        <view class="p-tit">{{name}}</view>
                        <view class="p-type">{{comment ? comment : ''}}{{comment2 ? comment2 : ''}}</view>
                        <block wx:if="{{anonymousState__temp15}}">
                            <view class="p-price">
                                <view class="p-price_low">¥{{' '}}{{anonymousState__temp6}}</view>
                                <block wx:if="{{is_group == 1}}">
                                    <view class="p-price_old">原价 ¥{{anonymousState__temp7}}</view>
                                </block>
                            </view>
                        </block>
                    </view>
                </view>
            </view>
            <view class="right-cn">
                <image class="icon-right" src="{{iconRight}}"></image><text class="right-text">正品保证 </text>
                <image class="icon-right" src="{{iconRight}}"></image><text class="right-text">退货保障 </text>
            </view>
            <block wx:if="{{seckillMode}}">
                <view class="bg-test-fill-radial-gradient seckill">
                    <view class="left layout ll">
                        <view class="price">￥19.9</view>/3包
                        <view class="tip">500组限量秒杀</view>
                        <view class="old-price">原价 ￥43
                            <view class="line"></view>
                        </view>
                    </view>
                    <view class="left layout lr">
                        <view class="title">距结束仅剩</view>
                        <countdown countDownDate="{{anonymousState__temp8}}" __triggerObserer="{{ _triggerObserer }}"></countdown>
                        <view class="arrow"></view>
                    </view>
                </view>
            </block>
            <block wx:if="{{anonymousState__temp16}}">
                <view class="{{groupTotalNum == 0 || is_group == 0 ? 'group-buy hide' : 'group-buy'}}">
                    <view class="gb-cn">
                        <view class="gb-item gb-tit">
                            <view class="l-cn">{{groupTotalNum}}人正在组队，可直接参与</view>
                            <view class="r-cn" bindtap="openDialog">查看更多
                                <at-icon value="chevron-right" __triggerObserer="{{ _triggerObserer }}" size="17" color="#959595"></at-icon>
                            </view>
                        </view>
                        <view class="gb-item" wx:key="$original.code" wx:for="{{loopArray1}}" wx:for-item="gb" wx:for-index="__index0">
                            <view class="l-cn">
                                <view class="avater-wrap">
                                    <block>
                                        <block wx:if="{{!gb.IDStaus}}">
                                            <image class="avater-pic" src="{{gb.$original.avatar}}" mode="widthFix"></image>
                                        </block>
                                        <block wx:else>
                                            <open-data type="userAvatarUrl" __triggerObserer="{{ _triggerObserer }}"></open-data>
                                        </block>
                                    </block>
                                </view>
                                <view class="nickName">
                                    <block>
                                        <block wx:if="{{!gb.IDStaus}}">{{gb.$original.name}}</block>
                                        <block wx:else>
                                            <open-data type="userNickName" __triggerObserer="{{ _triggerObserer }}"></open-data>
                                        </block>
                                    </block>
                                </view>
                            </view>
                            <view class="r-cn">
                                <view class="count-down">
                                    <view class="lp">还差<text class="num">{{gb.$original.left}}人</text>组队成功</view>
                                    <view class="lp kmCountDown">剩余
                                        <at-countdown format="{{gb.$loopState__temp4}}" __triggerObserer="{{ _triggerObserer }}" hours="{{gb.$original.ct ? gb.$original.ct.h : 0}}" minutes="{{gb.$original.ct ? gb.$original.ct.m : 1}}" seconds="{{gb.$original.ct ? gb.$original.ct.s : 59}}" bindontimeup="onTimeUp" __fn_onTimeUp="{{true}}" data-e-ontimeup-so="this" data-e-ontimeup-a-a="{{gb.$original.code}}"></at-countdown>
                                    </view>
                                </view>
                                <view class="join-btn" catchtap="anonymousFunc0" data-e-tap-so="this" data-e-tap-a-a="{{__index0}}">去组队</view>
                            </view>
                        </view>
                    </view>
                </view>
            </block>
            <view class="detail-pic">
                <block wx:if="{{!(goodInfo.id == 26 || goodInfo.id == 27)}}">
                    <view class="fc" bindtap="linkStar"></view>
                </block>
                <image class="detail-pic-img" src="{{url.$original}}" mode="widthFix" wx:key="$loopState__temp2" wx:for="{{loopArray0}}" wx:for-item="url" wx:for-index="_anonIdx"></image>
            </view>
        </view>
        <view class="{{isIphoneX ? 'fix-footer' : 'fix-footer noipx'}}">
            <view class="footer-cn">
                <view class="d-toolBar">
                    <view class="tab-home" catchtap="anonymousFunc3">
                        <image src="{{iconHome}}" class="tab-home-img"></image>
                        <view class="tab-text">首 页</view>
                    </view>
                    <block wx:if="{{anonymousState__temp17}}">
                        <view data-soldout="{{anonymousState__temp9}}" class="{{anonymousState__temp10}}" catchtap="handleBtnSecSnapClick">{{anonymousState__temp11}}</view>
                    </block>
                    <block wx:if="{{is_group == 0}}">
                        <view class="{{anonymousState__temp12}}" catchtap="handleBtnLjqClick">
                            <view class="tab-textb">立即抢</view>
                        </view>
                    </block>
                    <block wx:if="{{anonymousState__temp18}}">
                        <view class="tab-team" style="background-image: url(https://mpmall.huggiesapp.cn/storage/wxmall/another/detail_tabBar_06.png)" catchtap="anonymousFunc4">
                            <view class="tab-price">¥{{anonymousState__temp13}}</view>
                            <view class="tab-text">{{member_count ? member_count : 2}}人组队</view>
                        </view>
                    </block>
                    <block wx:if="{{anonymousState__temp19}}">
                        <view class="tab-pay" style="background-image: url(https://mpmall.huggiesapp.cn/storage/wxmall/another/detail_tabBar_05.png)" catchtap="anonymousFunc5">
                            <view class="tab-price">¥{{anonymousState__temp14}}</view>
                            <view class="tab-text">单人买</view>
                        </view>
                    </block>
                    <view class="tab-add-cart" catchtap="anonymousFunc6">
                        <image src="{{iconTabCart}}" class="tab-home-img"></image>
                        <view class="tab-text">加购物车</view>
                    </view>
                </view>
            </view>
        </view>
        <view class="{{isOpenSelectType ? 'dialog vi' : 'dialog'}}">
            <view class="dialog-layout">
                <view class="mask"></view>
                <view class="{{isIphoneX ? 'dialog-wrap' : 'dialog-wrap noipx'}}">
                    <view class="dialog-cn">
                        <view class="close" catchtap="anonymousFunc7">
                            <image src="{{iconClose}}" class="icon-close"></image>
                        </view>
                        <view class="select-body">
                            <view class="lab-item">
                                <view class="lab-tit">选择型号</view>
                                <view class="lab-cn">
                                    <at-tag name="{{t.id}}" __triggerObserer="{{ _triggerObserer }}" type="primary" wx:key="id" active="{{t.id == curTTag.id ? true : false}}" bindonclick="onClickTypeTag" wx:for="{{tTagsArr}}" wx:for-item="t" wx:for-index="_anonIdx2" __fn_onClick="{{true}}" data-e-onclick-so="this">{{t.tit}}</at-tag>
                                </view>
                            </view>
                            <view class="lab-item">
                                <view class="lab-tit">购买数量</view>
                                <view class="lab-cn">
                                    <view class="cover"></view>
                                    <at-input-number wx:key="userAction" __triggerObserer="{{ _triggerObserer }}" min="{{userAction == 'createTeam' ? buy_count : 1}}" max="{{100000}}" step="{{1}}" value="{{userAction == 'createTeam' ? groupNum : goodNum}}" bindonchange="anonymousFunc8" __fn_onChange="{{true}}"></at-input-number>
                                </view>
                            </view>
                        </view>
                        <view catchtap="anonymousFunc9" class="select-footer" style="background-image: url(https://mpmall.huggiesapp.cn/storage/wxmall/another/detail_tabBar_05.png)">确定</view>
                    </view>
                </view>
            </view>
        </view>
        <at-modal isOpened="{{isOpened}}" __triggerObserer="{{ _triggerObserer }}" className="kmDialog" wx:key="joinTeamDialog">
            <at-modal-header __triggerObserer="{{ _triggerObserer }}">正在组队
                <view class="icon-wrap">
                    <image src="{{iconClose}}" class="icon-close" bindtap="closeDialog"></image>
                </view>
            </at-modal-header>
            <at-modal-content __triggerObserer="{{ _triggerObserer }}">
                <view class="kmDialogWrap">
                    <view class="gb-item" wx:key="$original.code" wx:for="{{loopArray1}}" wx:for-item="gb" wx:for-index="__index0">
                        <view class="l-cn">
                            <view class="avater-wrap">
                                <block>
                                    <block wx:if="{{!gb.IDStaus}}">
                                        <image class="avater-pic" src="{{gb.$original.avatar}}" mode="widthFix"></image>
                                    </block>
                                    <block wx:else>
                                        <open-data type="userAvatarUrl" __triggerObserer="{{ _triggerObserer }}"></open-data>
                                    </block>
                                </block>
                            </view>
                            <view class="nickName">
                                <block>
                                    <block wx:if="{{!gb.IDStaus}}">{{gb.$original.name}}</block>
                                    <block wx:else>
                                        <open-data type="userNickName" __triggerObserer="{{ _triggerObserer }}"></open-data>
                                    </block>
                                </block>
                            </view>
                        </view>
                        <view class="r-cn">
                            <view class="count-down">
                                <view class="lp">还差<text class="num">{{gb.$original.left}}人</text>组队成功</view>
                                <view class="lp kmCountDown">剩余
                                    <at-countdown format="{{gb.$loopState__temp4}}" __triggerObserer="{{ _triggerObserer }}" hours="{{gb.$original.ct ? gb.$original.ct.h : 0}}" minutes="{{gb.$original.ct ? gb.$original.ct.m : 1}}" seconds="{{gb.$original.ct ? gb.$original.ct.s : 59}}" bindontimeup="onTimeUp" __fn_onTimeUp="{{true}}" data-e-ontimeup-so="this" data-e-ontimeup-a-a="{{gb.$original.code}}"></at-countdown>
                                </view>
                            </view>
                            <view class="join-btn" catchtap="anonymousFunc0" data-e-tap-so="this" data-e-tap-a-a="{{__index0}}">去组队</view>
                        </view>
                    </view>
                </view>
            </at-modal-content>
            <at-modal-action __triggerObserer="{{ _triggerObserer }}">仅显示10个正在组队的人</at-modal-action>
        </at-modal>
        <rule-dialog __triggerObserer="{{ _triggerObserer }}"></rule-dialog>
    </view>
</block>