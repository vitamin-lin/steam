<block>
    <view class="order">
        <view class="tits"></view>
        <view class="fc" style="{{anonymousState__temp}}"></view>
        <view class="tabs">
            <at-tabs current="{{current}}" __triggerObserer="{{ _triggerObserer }}" tabList="{{anonymousState__temp2}}" bindonclick="handleClick" __fn_onClick="{{true}}" data-e-onclick-so="this"></at-tabs>
            <view class="lines"></view>
        </view>
        <view class="order_box">
            <view class="main">
                <order-list list="{{list}}" __triggerObserer="{{ _triggerObserer }}" bindonbotombox="onBotomBox" bindontopbox="onTopBox" keys="{{current}}" bindonchangepay="onChangePay" visibleReback="{{orderNumber && current == 1}}" rebackWhere="{{buyType}}" __fn_onBotomBox="{{true}}" __fn_onTopBox="{{true}}" __fn_onChangePay="{{true}}"></order-list>
            </view>
        </view>
    </view>
</block>