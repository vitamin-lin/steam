<block>
    <view class="banner" style="{{anonymousState__temp}}">
        <swiper class="test-h" indicator-color="#999" indicator-active-color="#333" circular="{{true}}" indicator-dots="{{true}}" autoplay="{{true}}" style="{{anonymousState__temp2}}">
            <swiper-item wx:key="key" class="test-h" wx:for="{{list}}" wx:for-item="key" wx:for-index="_anonIdx">
                <image src="{{key}}" mode="widthFix"></image>
            </swiper-item>
        </swiper>
        <view class="pic">
            <view class="share" bindtap="funPrivateLuPOq" data-e-tap-so="this" data-e-tap-a-a="{{gid}}"></view>
            <block wx:if="{{!((id == 10 || id == 9) && id !== 11)}}">
                <view class="linkToDetail" bindtap="linkToDetail" data-e-tap-so="this" data-e-tap-a-a="{{gid}}" data-e-tap-a-b="{{id}}"></view>
            </block>
            <block wx:if="{{id == 11}}">
                <view class="linkWrap">
                    <view class="linkToDetails" bindtap="linkToDetail" data-e-tap-so="this" data-e-tap-a-a="{{8}}" data-e-tap-a-b="{{id}}"></view>
                    <view class="linkToDetails" bindtap="linkToDetail" data-e-tap-so="this" data-e-tap-a-a="{{26}}" data-e-tap-a-b="{{id}}"></view>
                    <view class="linkToDetails" bindtap="linkToDetail" data-e-tap-so="this" data-e-tap-a-a="{{27}}" data-e-tap-a-b="{{id}}"></view>
                </view>
            </block>
            <view wx:key="keys" class="details" wx:for="{{details}}" wx:for-item="keys" wx:for-index="indexs">
                <block>
                    <block wx:if="{{indexs == 1}}">
                        <view class="special">
                            <image src="{{keys}}" mode="widthFix"></image>
                            <image src="{{heart}}" mode="widthFix" class="hearts"></image><text class="nums">{{total}}</text>
                        </view>
                    </block>
                    <block wx:else>
                        <image src="{{keys}}" mode="widthFix"></image>
                    </block>
                </block>
            </view>
        </view>
    </view>
</block>